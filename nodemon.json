{
    "watch": [
        "sflo-host/dist",
        "plugins/*/dist",
        "shared/*/dist"
    ],
    "ext": "js,json",
    "ignore": [
        "node_modules",
        "**/*.test.ts",
        "**/*.spec.ts"
    ],
    "exec": "NODE_ENV=development node --inspect sflo-host/dist/index.js",
    "verbose": true
}
